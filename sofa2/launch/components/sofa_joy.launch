<launch>
    <node pkg="joy" name="joy" type="joy_node" output="screen">
        <param name="autorepeat_rate" value="10"/>
    </node>
    
    <node pkg="teleop_twist_joy" name="joy_twist" type="teleop_node" output="screen">
        <remap from="cmd_vel" to="command_in"/>
    </node>

    <node pkg="cob_collision_velocity_filter" name="cob_collision_velocity_filter" type="collision_velocity_filter" output="screen">
        <remap from="obstacles" to="move_base/local_costmap/costmap"/>
        <remap from="command" to="cmd_vel"/>
    </node>
</launch>